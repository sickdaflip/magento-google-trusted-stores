{
  "name": "sickdaflip/magento-google-trusted-stores",
  "type": "magento-module",
  "license": "MIT",
  "description": "Google Trusted Stores for Magento 1.9.3.",
  "authors": [
    {
      "name": "SickDaFlip",
      "email": "philippbreitsprecher@gmail.com"
    }
  ],
  "extra": {
    "map": [
      [
        "code",
        "app/code/community/Develo/Googletrustedstores/"
      ],
      [
        "Develo_Googletrustedstores.xml",
        "app/etc/modules/Develo_Googletrustedstores.xml"
      ],
      [
        "en_US/*",
        "app/locale/en_US/"
      ],
      [
        "frontend/layout/*",
        "app/design/frontend/base/default/layout/"
      ],
      [
        "frontend/template",
        "app/design/frontend/base/default/template/develo-googletrustedstores"
      ]
    ]
  }
}